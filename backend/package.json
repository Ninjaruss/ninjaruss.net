{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "cd ../frontend && yarn install",
    "start": "node ./bin/www",
    "devStart": "nodemon --inspect ./bin/www",
    "build": "cd ../frontend && yarn install && yarn run build",
    "client": "cd ../frontend && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\" "
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^7.5.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "jade": "~1.11.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.5",
    "morgan": "~1.9.1",
    "nodemailer": "^6.9.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20"
  }
}
